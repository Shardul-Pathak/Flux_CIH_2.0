{
  "manifest_version": 3,
  "name": "Privacy Policy Analyzer",
  "version": "1.0",
  "description": "Analyzes website privacy policies using the Gemini API.",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png"
    }
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}